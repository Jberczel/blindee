<script>
$(document).ready( function() {
var ctx = $(".myChart").get(0).getContext("2d");
// This will get the first returned node in the jQuery collection.

console.log( "<%= @results.keys %>" );

var data = {
    labels: <%= raw @results.keys %>,
    datasets: [
        {
            label: "My First dataset",
            fillColor: "rgba(165, 67, 153, 0.5)",
            strokeColor: "rgba(220, 191, 219, 0.8)",
            highlightFill: "rgba(165, 67, 153, 0.75)",
            highlightStroke: "rgba(220, 191, 219, 1)",
            data: <%= @results.values %>,
        },
        
    ]
};

var overides = {
    responsive: true,
    //maintainAspectRatio: false,
    scaleFontSize: 12,
    //barShowStroke : false

};

var myBarChart = new Chart(ctx).Bar(data, overides);
});
</script>