<h1>Editing vote</h1>

<%= render 'form' %>

<br>
<%= form_for @invite , :url => invites_path do |f| %>
    <%= f.hidden_field :vote_id, :value => @invite.vote_id %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.submit 'Send' %>
<% end %>

<%= link_to 'Show', @vote %> |
<%= link_to 'Back', votes_path %>
